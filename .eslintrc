{
  "extends": "eslint:recommended",
  "env": {
    "browser": false,
    "node": true,
    "es6": true,
    "jest": true
  },
  "plugins": [
    "node"
  ],
  "rules": {
    "no-console": "off",

    "node/no-unsupported-features": ["error", {"version": 4}],
    "node/no-missing-require": "error"
  }
}
